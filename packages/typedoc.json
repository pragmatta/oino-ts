{
    "name": "OINO TS",
    "entryPoints": ["./bunsqlite/*.ts", "./mariadb/*.ts",  "./postgresql/*.ts", "./core/src/index.ts"],
    "entryPointStrategy": "resolve",
    "sortEntryPoints": true,
    "skipErrorChecking": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "validation": {
        "invalidLink": true,
        "notDocumented": true,
        "notExported": true
    },
    "out": "../docs",
    "exclude": ["**/node_modules/**", "**/*.test.ts"],
    "requiredToBeDocumented": [
        "Enum",
        "Variable",
        "Function",
        "Class",
        "Interface",
        "Property",
        "Method",
        "Accessor",
        "TypeAlias",
        "Parameter"
    ]
}