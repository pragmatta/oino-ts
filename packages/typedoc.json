{
    "name": "OINO TS",
    "entryPoints": ["./bunsqlite/*.ts", "./mariadb/*.ts",  "./postgresql/*.ts", "./core/src/index.ts"],
    "entryPointStrategy": "resolve",
    "sortEntryPoints": true,
    "skipErrorChecking": true,
    "excludePrivate": true,
    "excludeProtected": true,
    "out": "../docs",
    "exclude": ["**/node_modules/**", "**/*.test.ts"]
}